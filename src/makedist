cp cdd.h cdd.h.dev
cp setoper.h setoper.h.dev
cp cddio.c cddio.c.dev
cp cddlib.c cddlib.c.dev
cp cddcore.c cddcore.c.dev
cp cddlp.c cddlp.c.dev
cp cddmp.c cddmp.c.dev
cp setoper.c setoper.c.dev
sed -e 's/#include "debugmem.h"//' cdd.h.dev > cdd.h
sed -e 's/#include "debugmem.h"//' setoper.h.dev > setoper.h
sed -e 's/debug_//g' cddio.c.dev > cddio.c
sed -e 's/debug_//g' cddlib.c.dev > cddlib.c
sed -e 's/debug_//g' cddcore.c.dev > cddcore.c
sed -e 's/debug_//g' cddlp.c.dev > cddlp.c
sed -e 's/debug_//g' cddmp.c.dev > cddmp.c
sed -e 's/debug_//g' setoper.c.dev > setoper.c
rm -f PB*
rm -f debugmem.h
rm -f ../makedir/debugmem.h
rm -rf obj
rm -rf English.lproj
rm -rf Makefile*
rm gmp.h
